  {
    "name": "Accessibility",
    "settings": [
      {
        "type": "text",
        "id": "accessibility_statement",
        "label": "Statement text",
        "default": "Accessibility statement"
      },
      {
        "type": "page",
        "id": "accessibility_statement_page",
        "label": "Accessibility statement page"
      }
    ]
