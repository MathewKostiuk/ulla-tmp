        "label": "Menu 9"
      },
      {
        "type": "link_list",
        "id": "site_map_10",
        "label": "Menu 10"
      }
    ]
  },
  {
    "name": "Searchspring",
    "settings": [
      {
        "type": "checkbox",
        "id": "ss_enable",
        "label": "Enable Searchspring",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "ss_search_only",
        "label": "Search only",
        "default": true
      },
      {
        "type": "text",
        "id": "ss_site_id",
        "label": "Site ID",
        "default": "g7562x"
      },
      {
        "type": "text",
        "id": "ss_collection_handle",
